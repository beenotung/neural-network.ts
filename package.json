{
  "name": "neural-network.ts",
  "version": "1.0.2",
  "description": "Lightweight TypeScript neural network library with back-propagation and genetic algorithms, supporting a wide range of activation functions, all without extra build or native dependencies.",
  "keywords": [
    "neural-network",
    "machine-learning",
    "genetic-algorithm",
    "supervised-learning",
    "reinforcement-learning",
    "typescript",
    "isomorphic",
    "activation-functions",
    "standalone",
    "export",
    "import",
    "nodejs",
    "browser",
    "lightweight",
    "AI"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm.js",
  "browser": "dist/browser.js",
  "unpkg": "dist/browser.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ts-mocha *.{spec,test}.ts",
    "coverage": "nyc npm test",
    "build": "run-s clean transpile",
    "clean": "rimraf dist",
    "transpile": "run-p esbuild:* tsc",
    "esbuild:browser": "esbuild --bundle --outfile=dist/browser.js browser.ts",
    "esbuild:esm": "esbuild --bundle --outfile=dist/esm.js --platform=node --format=esm index.ts",
    "tsc": "tsc -p ."
  },
  "dependencies": {
    "ga-island": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.11",
    "chai": "^4.4.1",
    "debug": "^4.3.5",
    "esbuild": "^0.23.0",
    "mocha": "^10.6.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "oklab.ts": "^2.2.6",
    "rimraf": "^5.0.9",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
